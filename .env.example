# 日志清理配置
CLEAN_LOG_ON_START=true
CLEAN_MEMORY_ON_START=true
MEMORY_RETENTION_COUNT=5

# OpenAI API配置
API_URL=您的API地址
API_KEY=您的API密钥
MODEL_NAME=gemini-2.0-flash
MAX_TOKENS=8192  # 最大令牌数

# 新增目录限制配置
ALLOWED_PATHS=./workspace  # 允许操作的路径（逗号分隔）
ALLOW_OUTSIDE_WORKSPACE=false  # 是否允许操作workspace外的文件

RATE_LIMIT_PER_MINUTE=60  # 每分钟最大请求数（-1表示无限制）
RATE_LIMIT_WAIT=60        # 达到限制后的等待时间（秒）